---
import type { HTMLAttributes } from "astro/types";
import Wrapper from "../Base/Wrapper.astro";

interface Props extends HTMLAttributes<'h1'> {}


const {
    class : className,
    ...rest
} = Astro.props

---

<h1 class:list={[`text-3xl font-semibold tracking-wide font-theme`, className]} {...rest}>
        <slot />
</h1>
